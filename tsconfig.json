{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "types": ["@cloudflare/workers-types"],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./app/src",
    "outDir": "./app/build"
  }
}
